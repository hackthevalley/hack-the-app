{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier":
        [ 
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off",
        "react/require-default-props": "off"
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true
    }
}
